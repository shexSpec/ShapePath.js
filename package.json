{
  "name": "ShapePath.js",
  "version": "0.0.1",
  "description": "A typescript library for executing ShapePaths",
  "main": "ShapePathJison.js",
  "type": "package",
  "scripts": {
    "all": "npm run parser && npm run test",
    "parser": "./node_modules/@ts-jison/parser-generator/lib/cli.js -n ShapePath -t typescript -o ShapePathJison.ts ShapePathJison.jison",
    "compile": "tsc ShapePathJison.ts",
    "run": "node ShapePathJison.js txt",
    "test": "ts-node ShapePathJison.ts txt",
    "compile2": "tsc",
    "test2": "jest",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest --runInBand ./__tests__/static-test.ts",
    "test3999": "npm run parser && npm run compile && npm run test2",
    "test3": "tsc --noEmit ShapePathJison.ts && npm run test2",
    "tyson": "tyson ./ShapePathJison.jison ./ShapePath.typedict.ts --out ./ShapePath.typecheck.ts && ./fixupNames ./ShapePath.typecheck.ts && tsc --noEmit ./ShapePath.typecheck.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shexSpec/ShapePath.js.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shexSpec/ShapePath.js/issues"
  },
  "homepage": "https://github.com/shexSpec/ShapePath.js#readme",
  "devDependencies": {
    "@ts-jison/parser-generator": "^0.0.5",
    "@types/node": "^14.14.28",
    "@types/shexj": "^2.1.0",
    "ts-jest": "^26.5.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2",
    "tyson": "^1.7.0"
  },
  "dependencies": {
    "@ts-jison/parser": "^0.0.5"
  }
}
